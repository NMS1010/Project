@{
    ViewBag.Title = "";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="card">
    <div class="card-header">
        <div>
            <h3 class="card-title"> Thống kê </h3>
        </div>
    </div>
    <div class="card-body">
        <table id="dataTable" class="table table-bordered  dataTable">
            <thead>
                <tr class="text-center">
                    <th style="text-align: center; width: 5%">Số nhà cung cấp </th>
                    <th style="text-align: center; width: 15%">Số loại Vắc Xin </th>
                    <th style="text-align: center; width: 15%">Số nhân viên </th>
                    <th style="text-align: center; width: 15%">Số khách hàng</th>
                    <th style="text-align: center; width: 15%">Tổng doanh thu</th>
                </tr>
            </thead>
            <tbody>

                <tr>
                    <td style="text-align:center">
                        @ViewBag.soncc
                    </td>
                    <td style="text-align:center">
                        @ViewBag.sovx
                    </td>
                    <td style="text-align:center">
                        @ViewBag.sonv
                    </td>
                    <td style="text-align:center">
                        @ViewBag.sokh
                    </td>
                    <td style="text-align:center">
                        <b>@ViewBag.TongTien Vnd</b>
                    </td>
                </tr>
            </tbody>
        </table>
    </div>
</div>









